module digit(n, h) {
	if (n >= 0 && n <= 9) {
		if (n == 1) {
			digit_1(h);
		} else if (n == 2) {
			digit_2(h);
		} else if (n == 3) {
			digit_3(h);
		} else if (n == 4) {
			digit_4(h);
		} else if (n == 5) {
			digit_5(h);
		} else if (n == 6) {
			digit_6(h);
		} else if (n == 7) {
			digit_7(h);
		} else if (n == 8) {
			digit_8(h);
		} else if (n == 9) {
			digit_9(h);
		} else {
			digit_0(h);
		}
	} else {
		echo ("ERROR: digit(n, h): n must be between 0 and 9.");
	}
}

module franc_digit(n, h) {
	if (n >= 0 && n <= 9) {
		digit(n, h);
	} else {
		echo ("ERROR: franc_digit(n, h): n must be between 0 and 9.");
	}
}

module cents_digits(n, h) {
	if (n >= 0 && n <= 99) {
		// Dot
		translate([-digit_frame_width/2, -2*digit_frame_height/5, 0]) {
      cube([2, 2, h]);
    }

    if (n == 0) {
			// Minus
      translate([-digit_frame_width/4, -digit_frame_width/4, 0]) {
        cube([digit_frame_width/2, 2, h]);  
      }
		} else {
			translate([-digit_frame_width/4, 0, 0]) {
        scale([0.5, 0.5, 1]) {
          // First digit
          digit(floor(n/10), h);
        }
      }

      translate([digit_frame_width/4, 0, 0]) {
        scale([0.5, 0.5, 1]) {
          // Second digit
          digit(n - 10*floor(n/10), h);
        }
      }
		}
	} else {
		echo ("ERROR: cents_digits(n, h): n must be between 0 and 99.");
	}
	
}